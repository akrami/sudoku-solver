            crossorigin="anonymous"></script>
    <title>{{ title??"Sudoku Solver" }}</title>
</head>
<body>
<div class="pure-g">
    <div class="pure-u-1 pure-u-lg-4-5 container">
        {% block body %}{% endblock %}
    </div>
</div>

<script>
    feather.replace();
    {% block javascript %}{% endblock %}
</script>
</body>
</html>